MOD_NAME=PaintBGEE
PORTRAIT_VERSION=2
CODE_VERSION=0.4

if [ "$PORTRAIT_VERSION" ]; then
    MOD_VERSION="$CODE_VERSION-$PORTRAIT_VERSION"
else
    MOD_VERSION="$CODE_VERSION"
fi

MOD_ARTISTS=artastrophe
MOD_BUILDER=polymorphedsquirrel

if [ "$MOD_ARTISTS" ]; then 
    MOD_AUTHORS="art: $MOD_ARTISTS; bugs: $MOD_BUILDER"
else
    MOD_AUTHORS="$MOD_BUILDER"
fi

